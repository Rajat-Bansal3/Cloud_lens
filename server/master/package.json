{
	"name": "cloudlens",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon",
		"start": "node dist/index.js",
		"start:release": "NODE_ENV=production node dist/index.js",
		"build": "tsc",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"type-check": "tsc --noEmit",
		"audit": "bun audit",
		"update-deps": "bun outdated && bun update",
		"clean": "rm -rf dist node_modules && bun cache clean --force",
		"reset": "bun run clean && bun install",
		"checklines": "bun run clean && cloc . && bun install"
	},
	"keywords": [],
	"author": "Rajat Bansal",
	"license": "ISC",
