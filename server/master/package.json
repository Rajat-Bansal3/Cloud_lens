		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.1",
		"@types/helmet": "^0.0.48",
		"@types/mongoose": "^5.11.97",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.13.10",
		"@types/pino-multi-stream": "^5.1.6",
