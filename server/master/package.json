		"@aws-sdk/client-cost-explorer": "^3.797.0",
		"@aws-sdk/client-lambda": "^3.797.0",
		"@types/jsonwebtoken": "^9.0.9",
		"axios": "^1.8.2",
		"bcryptjs": "^3.0.2",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^5.1.0",
		"helmet": "^8.0.0",
		"mongoose": "^8.12.1",
		"morgan": "^1.10.0",
		"pino": "^9.6.0",
		"pino-http": "^10.4.0",
		"pino-multi-stream": "^6.0.0",
		"pino-pretty": "^13.0.0",
